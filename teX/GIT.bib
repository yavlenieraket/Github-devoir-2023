@article{stutzmann_paleographie_2011,
	title = {Paléographie statistique pour décrire, identifier, dater... {Normaliser} pour coopérer et aller plus loin ?},
	url = {https://shs.hal.science/halshs-00596970},
	abstract = {Les initiatives TEI (Text Encoding Initiative) et MUFI (Medieval Unicode Font Initiative) multiplient les potentialités des transcriptions et invitent à interroger le processus même d'une opération pourtant commune dans les études médiévales. Transcrire, c'est décrire un texte et sa mise en forme graphique. Cela ouvre des voies aux études paléographiques, notamment par la description des diverses variantes morphologiques des lettres ou abréviations. Des recherches entreprises sur la production écrite de l'abbaye cistercienne de Fontenay démontrent le potentiel considérable des transcriptions dites " allographétiques " et des statistiques descriptives pour comprendre les évolutions de l'écriture médiévale. La fragmentation du paysage actuel de la recherche amène pourtant à souhaiter une harmonisation des pratiques, tant pour décrire les phénomènes paléographiques que pour structurer les données et formuler des ontologies.},
	language = {fr},
	urldate = {2023-03-29},
	author = {Stutzmann, Dominique},
	year = {2011},
	note = {Publisher: BoD},
	pages = {247},
	file = {Full Text PDF:C\:\\Users\\freij\\Zotero\\storage\\MSREMD9L\\Stutzmann - 2011 - Paléographie statistique pour décrire, identifier,.pdf:application/pdf},
}
